    <resource-file src="noti-icon.png" target="resources\android\icon\drawable-ldpi-icon.png" />
<resource-file src="noti-icon-hdpi.png" target="resources\android\icon\drawable-hdpi-icon.png" />
  </platform>
  <platform name="android">
    <icon src="resources/android/icon/ldpi.png" density="ldpi"/>
    <icon src="resources/android/icon/mdpi.png" density="mdpi"/>
    <icon src="resources/android/icon/hdpi.png" density="hdpi"/>
